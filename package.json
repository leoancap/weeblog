{
  "name": "bloggy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --port 3000",
    "build": "parcel build src/index.html -d public --no-source-maps",
    "test": "jest -c jest.config.js",
    "test:watch": "jest -c jest.config.js --watchAll",
    "test:coverage": "npm run test -- --watchAll false --coverage"
  },
  "alias": {
    "#root": "./src",
    "#actions": "./src/store/actions",
    "#components": "./src/components",
    "#constants": "./src/constants",
    "#customHooks": "./src/customHooks",
    "#domainTypes": "./src/types",
    "#pages": "./src/pages",
    "#router": "./src/router",
    "#selectors": "./src/selectors",
    "#services": "./src/services",
    "#store": "./src/store",
    "#storeTypes": "./src/store/types",
    "#styled": "./src/styled",
    "#styledTypes": "./src/types/styledTypes"
  },
  "dependencies": {
    "formik": "^1.5.8",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-css-spinners": "^3.1.1",
    "react-dnd": "5.0.0",
    "react-dnd-html5-backend": "3.0.2",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-tag-input": "^6.4.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "@types/faker": "^4.1.5",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-tag-input": "^6.1.1",
    "@types/styled-components": "^4.1.18",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3"
  }
}
