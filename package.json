{
  "name": "bloggy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel public/index.html --port 3000",
    "build": "parcel build public/index.html --no-source-maps",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --watchAll false --coverage"
